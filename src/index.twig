{% import "templates/components/hero-title.twig" as title %}

{% extends 'templates/partials/base.twig' %}
{% block main %}
  <div class="container">
    <div id="hero" class="hero">
      <div class="hero-img-container position-absolute zindex-1">
        <img class="hero-img" src="./assets/images/hero.png" alt="digicraft-hero-image" />
        <div class="overlay position-absolute zindex-1"></div>
      </div>
      <div class="position-relative zindex-2">
        {{ title.stringValue('display-1','Creative Meets Technology.') }}
      </div>
      <button class="btn btn-primary">Primary buttons</button>
    </div>

    <div class="mt-5">
      <p>i am here</p>
      <img src="./assets/images/basf.svg" width="250" alt="" />
    </div>
  </div>
{% endblock %}
